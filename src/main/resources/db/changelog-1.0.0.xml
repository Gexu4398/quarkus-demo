<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

  <changeSet id="1736412137222-1" author="singhand">
    <createTable tableName="student">
      <column name="id" type="VARCHAR(255)">
        <constraints nullable="false" primaryKey="true" primaryKeyName="pk_student"/>
      </column>
      <column name="name" type="VARCHAR(255)">
        <constraints nullable="false"/>
      </column>
      <column name="createdat" type="TIMESTAMP WITHOUT TIME ZONE"/>
      <column name="updatedat" type="TIMESTAMP WITHOUT TIME ZONE"/>
    </createTable>
  </changeSet>
</databaseChangeLog>